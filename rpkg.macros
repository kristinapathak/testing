function git_short_hash {
    short_hash="$(cached git_short_hash)"

    if [ -z "$short_hash" ]; then
        short_hash="$(git rev-parse --short HEAD)"
    fi

    output "$short_hash"
}

function git_tag_version {
    tag="$(cached git_tag_version)"

    if [ -z "$tag" ]; then
        tag="$(git rev-parse --abbrev-ref HEAD)"
    fi

    output "$tag"
}